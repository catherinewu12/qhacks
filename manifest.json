{
"name": "NO SPOILERS",
"version": "1.0",
"manifest_version": 2,
"author": "B. Arthur, J. Laframboise, C. Wu, E. Zhao",
"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	}
],
"background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
},
"permissions": ["tabs", "storage"],
"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html",
	"default_title": "spoiler alert"
	}

}