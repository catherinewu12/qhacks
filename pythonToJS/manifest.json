{
"name": "Access Python",
"version": "1.0",
"manifest_version": 2,
"author": "us",
"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	}
],
"permissions": ["tabs"],
"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html",
	"default_title": "Access Python"
	}

}